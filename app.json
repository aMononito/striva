{
  "expo": {
    "name": "Striva",
    "slug": "striva",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/striva-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.strivainc.striva",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "android": {
      "package": "com.strivainc.striva"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6097d08d-b289-483e-a1c0-66a9ef9cdff4"
      }
    },
    "owner": "mononito"
  }
}
